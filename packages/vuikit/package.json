{
  "name": "vuikit",
  "version": "0.8.10",
  "description": "A responsive Vue UI library for web site interfaces based on UIkit",
  "license": "MIT",
  "author": {
    "name": "Miljan Aleksic",
    "url": "https://twitter.com/AleksicMiljan"
  },
  "homepage": "https://vuikit.js.org/",
  "bugs": "https://github.com/vuikit/vuikit/issues",
  "main": "dist/vuikit.cjs.js",
  "module": "dist/vuikit.esm.js",
  "keywords": [
    "library",
    "vue",
    "vuejs",
    "uikit",
    "vuikit"
  ],
  "files": [
    "src",
    "dist/*.js",
    "lib/**/*.js"
  ],
  "scripts": {
    "dev": "start-storybook -s ./.storybook/static -p 6006",
    "build:dist": "node -r esm scripts/build.js",
    "build:lib": "node -r esm scripts/build-lib.js",
    "build": "npm run build:lib && npm run build:dist",
    "postbuild": "node -r esm scripts/copyright.js",
    "test": "jest"
  },
  "dependencies": {
    "esm": "^3.2.25",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@miljan/build": "^0.4.2",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/vue": "^3.3.15",
    "@vue/test-utils": "^1.0.0-beta.12",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-vue": "^2.0.1",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "globby": "^8.0.1",
    "jest": "^22.4.2",
    "jest-serializer-vue": "^1.0.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "less-plugin-clean-css": "^1.5.1",
    "lodash": "^4.17.4",
    "replace-in-file": "^3.2.0",
    "rollup": "^0.57.0",
    "rollup-analyzer": "^1.1.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-cleanup": "^2.0.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "uikit": "^3.0.0-beta.40",
    "vue-jest": "^2.1.1",
    "vue-loader": "^14.2.1",
    "vue-server-renderer": "^2.5.16",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "globals": {
      "ENV": true
    },
    "env": {
      "jest": true
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "modulePaths": [
      "<rootDir>/../../node_modules",
      "<rootDir>/../../packages"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  }
}
